{
  "name": "jae-bradley-cli-creator",
  "version": "0.0.0-development",
  "description": "Generate a new CLI package from the command line, based on my personal preferences",
  "main": "./build/index.js",
  "bin": {
    "jb-create-cli": "./build/index.js"
  },
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --ext .js .",
    "test": "jest --coverage --passWithNoTests",
    "prepublishOnly": "npm run webpack-production",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "webpack-production": "NODE_ENV=production ./node_modules/.bin/webpack --config './configs/webpack.config.production'",
    "webpack-development": "NODE_ENV=development ./node_modules/.bin/webpack-dashboard -- ./node_modules/.bin/webpack --config './configs/webpack.config.development'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/jae-bradley-cli-creator.git"
  },
  "keywords": [
    "cli",
    "commander",
    "inquirer"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/jae-bradley-cli-creator/issues"
  },
  "homepage": "https://github.com/jaebradley/jae-bradley-cli-creator#readme",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/templates/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/prompt": "^7.1.2",
    "@commitlint/prompt-cli": "^7.1.2",
    "ajv": "^6.5.3",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "semantic-release": "^15.9.15",
    "shebang-loader": "^0.0.1",
    "travis-deploy-once": "^5.0.7",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@octokit/rest": "^15.11.2",
    "chalk": "^2.4.1",
    "child-process-promise": "^2.2.1",
    "commander": "^2.18.0",
    "email-validator": "^2.0.4",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "inquirer": "^6.2.0",
    "is-online": "^7.0.0",
    "mustache": "^3.0.0",
    "normalize-package-data": "^2.4.0",
    "sort-package-json": "^1.15.0",
    "untildify": "^3.0.3",
    "validate-npm-package-name": "^3.0.0"
  }
}
